{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "xv6: qemu-gdb",
      "type": "shell",
      "command": "make qemu-gdb",
      "isBackground": true,
      "presentation": {
        "reveal": "silent",
        "panel": "dedicated",
        "clear": false
      },
      "problemMatcher": {
        "owner": "xv6-qemu",
        "fileLocation": "absolute",
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": "qemu-system-i386 .*tcp::26000"
        },
        "pattern": {
          "regexp": ".^"
        }
      }
    }
  ]
}
